<module :title="<%=page.title%>">
	<template><%-page.content %><%-toc ( page.content, { class: "am", list_number: false } ) %></template>
	<script>
		new am.Module ( {
			init: function ( event ) {
				event.on ( "deliverToc", function ( obj ) {
					obj.list = [ "index", "index2" ];
				} );
			}
		} );
	</script>
</module>